<% @title = 'テスト情報登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @test_info ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '登録' %>
      <%= link_to 'キャンセル', controller: "test_infos", action: "index", param_id: @test_basic_id %>
    </div>
  <% end %>
</div>
