<%= form_for @search_form, as: 'search', url: :admin_accounts,
    html: { method: :get, class: 'search' } do |f| %>

  <%= markup do |m|
    p = FormPresenter.new(f, self)
    m << p.label(:account_name, '氏名検索')
    m.br
    m << p.text_field(:account_name)
    m << '&nbsp;'
    m << f.submit('検索')
  end %>

<% end %>
