<%= form_for @search_form, as: 'search', url: :admin_test_basic_infos,
    html: { method: :get, class: 'search' } do |f| %>

  <%= markup do |m|
    p = FormPresenter.new(f, self)
    m << p.label(:year_name, '年度名称検索')
    m.br
    m << p.text_field(:year_name)
    m << '&nbsp;'
    m << f.submit('検索')
  end %>

<% end %>
